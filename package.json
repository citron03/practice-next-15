{
  "private": true,
  "type": "module",
  "scripts": {
    "catalog:codemod": "node ./scripts/convert-to-catalog.mjs",
    "catalog:check": "node ./scripts/convert-to-catalog.mjs --dry",
    "catalog:apply": "node ./scripts/convert-to-catalog.mjs",
    "web": "pnpm --filter web",
    "web:dev": "pnpm run web dev",
    "ds": "pnpm --filter @citron03/design-system",
    "ds:dev": "pnpm run ds dev",
    "commit": "pnpm --filter scripts commit",
    "ready": "pnpm --filter scripts ready",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "lint-staged": {
    "*.ts": "pnpm exec eslint --fix",
    "*.tsx": "pnpm exec eslint --fix",
    "*.js": "pnpm exec eslint --fix",
    "*.jsx": "pnpm exec eslint --fix",
    "*.{ts,tsx,js,jsx}": "pnpm exec prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
